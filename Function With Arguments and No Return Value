/*This program is an example of a Function With Arguments and No Return Value
Written by: Joshua Miguel G. Jamisola
10/26/2023*/

#include<stdio.h>

void product(int x, int y);
{
    printf("product of %d and %d is: %d", x, y, x * y );

}

int main()
{
    int x, y;
    printf("Enter x and y\n");

    scanf("%d %d", &x, &y);

    product(x, y);
    
    return 0;
}
