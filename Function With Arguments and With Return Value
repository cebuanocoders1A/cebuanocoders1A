#include<stdio.h>

int product(int x, int y){return x * y;}

int main()
{
    int x, y
    printf("Enter x and y\n");
    scanf("%d %d", &x, &y);

    pritnf("product of %d is: %d", x, y, product (x * y));
    
    return 0;
    

}
